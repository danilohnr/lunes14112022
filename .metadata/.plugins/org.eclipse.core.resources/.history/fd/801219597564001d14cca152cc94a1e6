package poo;

public class Aula {
	private int ID;
	private final int MaxEstudiantes=20;
	private String Asignatura;
	
	private Profesor profesor;
	private Estudiantes[] estudiantes;
	
	
	public Aula() {
		this.ID=1;
		this.profesor=new Profesor();
		this.Estudiantes=new Estudiante[MaxEstudiantes];
	}

}
